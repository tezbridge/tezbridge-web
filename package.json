{
  "name": "tezbridge-web",
  "version": "1.0.0",
  "description": "The TezBridge web front-end",
  "main": "docs/index.html",
  "scripts": {
    "flow": "flow",
    "dev": "parcel src/pages/index.html --target browser --https",
    "test": "flow && parcel build test/index.js --target node -o test.js && node dist/test.js",
    "build": "rm -rf docs && rm -rf .cache && flow && parcel build src/pages/index.html --no-source-maps --out-dir docs  && cp src/CNAME docs/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tezbridge/tezbridge-web.git"
  },
  "author": "catsigma",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tezbridge/tezbridge-web/issues"
  },
  "homepage": "https://github.com/tezbridge/tezbridge-web#readme",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.15",
    "@fortawesome/free-solid-svg-icons": "^5.7.2",
    "@fortawesome/vue-fontawesome": "^0.1.5",
    "bootstrap": "^4.3.1",
    "bootstrap-vue": "^2.0.0-rc.13",
    "forms": "^1.3.0",
    "tezbridge-crypto": "^1.0.23",
    "tezbridge-network": "^1.0.8",
    "vue": "^2.6.8",
    "vue-hot-reload-api": "^2.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-transform-flow-strip-types": "^7.3.4",
    "@vue/component-compiler-utils": "^2.6.0",
    "cssnano": "^4.1.10",
    "flow-bin": "^0.89.0",
    "sass": "^1.17.2",
    "vue-template-compiler": "^2.6.8"
  }
}
