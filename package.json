{
  "name": "tezbridge-web",
  "version": "1.0.0",
  "description": "The TezBridge web front-end",
  "main": "docs/index.html",
  "scripts": {
    "flow": "flow",
    "dev": "parcel src/pages/index.html",
    "test": "flow && parcel build test/index.js --target node -o test.js && node dist/test.js",
    "build": "rm -rf docs && rm -rf .cache && flow && parcel build src/pages/index.html --no-source-maps --out-dir docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tezbridge/tezbridge-web.git"
  },
  "author": "catsigma",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tezbridge/tezbridge-web/issues"
  },
  "homepage": "https://github.com/tezbridge/tezbridge-web#readme",
  "dependencies": {
    "bootstrap": "^4.3.1",
    "bootstrap-vue": "^2.0.0-rc.13",
    "tezbridge-crypto": "^1.0.15",
    "tezbridge-network": "^1.0.8",
    "vue": "^2.6.6",
    "vue-hot-reload-api": "^2.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/plugin-transform-flow-strip-types": "^7.2.3",
    "@vue/component-compiler-utils": "^2.5.2",
    "cssnano": "^4.1.10",
    "flow-bin": "^0.89.0",
    "sass": "^1.17.1",
    "vue-template-compiler": "^2.6.6"
  }
